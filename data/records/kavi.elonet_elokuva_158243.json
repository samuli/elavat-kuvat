aitaa ja solmiota... surut pois ja kukka rintaan... Emil Elo Oy Mikonkatu 13.","topic":["pukeutuminen","muoti","vaatteet"],"url":["http://www.elonet.fi/fi/elokuva/158243","http://www.elonet.fi/fi/video/1672607_158243"],"thumbnail":"http://www.elonet.fi/tenho/media/1672607_digikuva_suoratoisto_thumbnail.jpg","author":["Uno Pihlström"],"author_role":["d02"],"author_sort":"Uno Pihlström","author2":["Reino Palmroth","Uno Pihlström"],"author2_role":["aus","cng"],"author_corporate":["Suomi-Filmi Oy"],"author_corporate_role":["e10"],"geographic":["Suomi"],"genre":["mainos"],"format":[{"value":"0/Video/","translated":"Video"},{"value":"1/Video/Short/","translated":"Elokuva, lyhyt"}],"main_date":"1953-01-01T00:00:00Z","publication_daterange":"[1953-01-01 TO 1953-12-31]","search_daterange_mv":["[1953-01-01 TO 1953-12-31]"],"author_facet":["Uno Pihlström","Reino Palmroth","Suomi-Filmi Oy"],"language":["fin"],"id":"kavi.elonet_elokuva_158243","is_hierarchy_id":"kavi.elonet_elokuva_158243","is_hierarchy_title":"EMIL ELO OY - ALKUPALA","first_indexed":"2019-09-30T22:00:14Z","last_indexed":"2021-04-30T06:33:56Z","publishDate":["1953"],"building":[{"value":"0/KAVI/","translated":"Kansallinen audiovisuaalinen instituutti"}],"institution":["KAVI"],"sector_str_mv":[{"value":"0/arc/","translated":"Arkisto"}],"topic_facet":["muoti","pukeutuminen","vaatteet"],"online_boolean":1,"genre_facet":["mainos"],"online_str_mv":["kavi"],"free_online_str_mv":["kavi"],"source_str_mv":["kavi"],"work_keys_str_mv":["AT pihlströmuno emilelooyalkupala"],"geographic_facet":["Suomi"],"author2_id_str_mv":["kavi.elonet_henkilo_102467","kavi.elonet_henkilo_157512","kavi.elonet_yhtio_111432","kavi.elonet_yhtio_157821"],"author2_id_role_str_mv":["kavi.elonet_henkilo_102467###cng","kavi.elonet_henkilo_102467###drt","kavi.elonet_henkilo_157512###aus","kavi.elonet_henkilo_157512###spk,","kavi.elonet_yhtio_111432###fmp","kavi.elonet_yhtio_157821###tilaaja"],"main_date_str":"1953","title_sort":"EMIL ELO OY - ALKUPAL