is rannasta. Myyjä punnitsee silakoita asiakkaalle Helsingin Kauppatorilla. Lokki lentää taivaalla.","topic":["kalastus","talvipyynti","Rymättylä","silakka","pyynti","nuotat","myynti","Helsinki","kauppatori"],"url":["http://www.elonet.fi/fi/elokuva/133509","http://www.elonet.fi/fi/video/1685496_133509"],"thumbnail":"http://www.elonet.fi/tenho/media/1685496_digikuva_suoratoisto_thumbnail.jpg","author":["Valentin Vaala"],"author_role":["d02"],"author_sort":"Valentin Vaala","author2":["Reijo Lås"],"author2_role":["cng"],"author_corporate":["Suomi-Filmi Oy"],"author_corporate_role":["e10"],"geographic":["Suomi"],"genre":["ammatinkuvaus","kansatieteellinen elokuva","dokumentti"],"format":[{"value":"0/Video/","translated":"Video"},{"value":"1/Video/Short/","translated":"Elokuva, lyhyt"}],"main_date":"1963-01-01T00:00:00Z","publication_daterange":"[1963-01-01 TO 1963-12-31]","search_daterange_mv":["[1963-01-01 TO 1963-12-31]"],"author_facet":["Valentin Vaala","Reijo Lås","Suomi-Filmi Oy"],"language":["fin"],"id":"kavi.elonet_elokuva_133509","is_hierarchy_id":"kavi.elonet_elokuva_133509","is_hierarchy_title":"Meri antaa","first_indexed":"2017-04-11T11:50:41Z","last_indexed":"2021-04-30T06:32:07Z","publishDate":["1963"],"building":[{"value":"0/KAVI/","translated":"Kansallinen audiovisuaalinen instituutti"}],"institution":["KAVI"],"sector_str_mv":[{"value":"0/arc/","translated":"Arkisto"}],"topic_facet":["Helsinki","Rymättylä","kalastus","kauppatori","myynti","nuotat","pyynti","silakka","talvipyynti"],"online_boolean":1,"genre_facet":["ammatinkuvaus","dokumentti","kansatieteellinen elokuva"],"online_str_mv":["kavi"],"free_online_str_mv":["kavi"],"source_str_mv":["kavi"],"work_keys_str_mv":["AT vaalavalentin meriantaa"],"geographic_facet":["Suomi"],"author2_id_str_mv":["kavi.elonet_henkilo_102816","kavi.elonet_henkilo_116491","kavi.elonet_yhtio_111432"],"author2_id_role_str_mv":["kavi.elonet_henkilo_102816###drt","kavi.elonet_henkilo_116491###cng","kavi.elonet_yhtio_111432###f