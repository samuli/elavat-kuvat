aa sen Sylville.","topic":["valtionpäämiehet","presidentit","Suomi","Kekkonen, Urho (1900-1986)","valtiovierailut","Tampere"],"url":["http://www.elonet.fi/fi/elokuva/166615","http://www.elonet.fi/fi/video/1528941_166615"],"thumbnail":"http://www.elonet.fi/tenho/media/1528941_digikuva_suoratoisto_thumbnail.jpg","author":["Harry Lewing","Allan Pyykkö","Orvo Saarikivi","Armas Laurinen"],"author_role":["d02"],"author_sort":"Harry Lewing","author2":["Jouko Loikkanen","Eino S. Repo","Niilo Heino"],"author2_role":["aus","cng"],"author_corporate":["Suomi-Filmi Oy"],"author_corporate_role":["e10"],"geographic":["Suomi"],"genre":["esittelyelokuva","henkilödokumentti","dokumentti"],"format":[{"value":"0/Video/","translated":"Video"},{"value":"1/Video/Short/","translated":"Elokuva, lyhyt"}],"main_date":"1961-01-01T00:00:00Z","publication_daterange":"[1961-01-01 TO 1961-12-31]","search_daterange_mv":["[1961-01-01 TO 1961-12-31]"],"author_facet":["Harry Lewing","Allan Pyykkö","Orvo Saarikivi","Armas Laurinen","Jouko Loikkanen","Eino S. Repo","Niilo Heino","Suomi-Filmi Oy"],"language":["fin"],"id":"kavi.elonet_elokuva_166615","is_hierarchy_id":"kavi.elonet_elokuva_166615","is_hierarchy_title":"Urho Kekkonen","first_indexed":"2017-04-11T11:56:37Z","last_indexed":"2021-04-30T06:31:05Z","publishDate":["1961"],"building":[{"value":"0/KAVI/","translated":"Kansallinen audiovisuaalinen instituutti"}],"institution":["KAVI"],"sector_str_mv":[{"value":"0/arc/","translated":"Arkisto"}],"topic_facet":["Kekkonen, Urho (1900-1986)","Suomi","Tampere","presidentit","valtionpäämiehet","valtiovierailut"],"online_boolean":1,"genre_facet":["dokumentti","esittelyelokuva","henkilödokumentti"],"online_str_mv":["kavi"],"free_online_str_mv":["kavi"],"source_str_mv":["kavi"],"work_keys_str_mv":["AT lewingharry urhokekkonen"],"geographic_facet":["Suomi"],"author2_id_str_mv":["kavi.elonet_henkilo_101559","kavi.elonet_henkilo_102466","kavi.elonet_henkilo_1067485","kavi.elonet_henkilo_107863","kavi.elonet_henk